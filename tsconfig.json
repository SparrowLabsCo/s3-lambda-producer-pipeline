{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
    "include": ["src/**/*.ts", "src/**/*.js"],
    "compilerOptions": {
      "lib": ["es2021"],
      "module": "commonjs",
      "target": "es2021",
      "outDir": "src/dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    }
}
